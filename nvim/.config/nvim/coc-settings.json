{
    "coc.preferences.previewAutoClose": true,
    "coc.preferences.useQuickfixForLocations": true,
    "diagnostic.messageTarget": "echo",
    "languageserver": {
        "bash": {
            "command": "bash-language-server",
            "args": [
                "start"
            ],
            "filetypes": [
                "sh"
            ],
            "ignoredRootPaths": [
                "~"
            ]
        },
        "dockerfile": {
            "command": "docker-langserver",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "dockerfile"
            ]
        },
        "golang": {
            "command": "gopls",
            "filetypes": [
                "go"
            ],
            "rootPatterns": [
                "go.mod",
                ".git/"
            ],
            "initializationOptions": {
                "usePlaceholders": true
            }
        },
        "lua": {
            "command": "lua-lsp",
            "filetypes": [
                "--lua"
            ]
        },
        "sourcekit-lsp": {
            "command": "sourcekit-lsp",
            "filetypes": [
                "swift"
            ]
        }
    },
    "python.analysis.diagnosticMode": "openFilesOnly",
    "python.analysis.diagnosticSeverityOverrides": {
        "reportGeneralTypeIssues": "none"
    },
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "~/.local/bin/black",
    "python.linting.flake8Path": "~/.local/bin/flake8",
    "python.linting.flake8Enabled": false,
    "python.linting.mypyEnabled": false,
    "python.jediEnabled": true,
    "python.sortImports.path": "/Users/shagohead/.local/bin/isort",
    "python.sortImports.args": [
        "--line-length",
        "88",
        "--multi-line",
        "3",
        "--force-grid-wrap",
        "0",
        "--trailing-comma"
    ],
    "python.workspaceSymbols.ctagsPath": "ctags.fish",
    "python.workspaceSymbols.tagFilePath": "${workspaceFolder}/.ctags",
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "function": "\uf794",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "typescript.suggestionActions.enabled": true
}
